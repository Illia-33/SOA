specification {
    element actor {
        style {
            shape person
        }
    }

    element service {
    }

    element database {
        style {
            shape storage
        } 
    }

    element broker {
        style {
            shape queue
        }
    }
}

model {
    user = actor {
        title 'User'
        description 'End-user interacting via UI or HTTP client'
    }

    socialNetworkApp = service {
        title 'Social Network'
        description 'Service-oriented social network application'
    }

    user -> socialNetworkApp 'Uses through UI'
}

views {
    view contextView {
        title 'Context view'
        include user, socialNetworkApp
    }
}
